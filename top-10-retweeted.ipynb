   "source": [
    "## Función para obtener los 10 tweets con más retweets"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "72ebee1a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def top_10_retweeted(raw_tweets):\n",
    "    raw_tweets = raw_tweets.sort_values(\"retweetCount\", ascending = False)\n",
    "    raw_tweets = raw_tweets[[\"id\"]].head(10)\n",
    "    return list(raw_tweets[\"id\"])"
   ]
  }
